<mvc:View controllerName="projectBP.controller.BusinessPartnerHome"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	displayBlock="true"
	height="100%">
	<ScrollContainer height="100%"
		width="100%"
		vertical="true"
		focusable="true">
		<Panel width="auto"
			class="sapUiSmallMargin">
			<content class="panelbottomline">
				<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop"
					header="고객 마스터 목록 조회"
					press="onDisplayBP">
					<TileContent >
						<ImageContent src="sap-icon://multiselect-all"></ImageContent>
					</TileContent>
				</GenericTile>

				<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop"
					header="고객 마스터 개인 생성(대량)"
					press="onCreateBP_person">
					<TileContent >
						<ImageContent src="sap-icon://create-form"></ImageContent>
					</TileContent>
				</GenericTile>

				<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop"
					header="고객 마스터 조직 생성(대량)"
					press="onCreateBP_organization">
					<TileContent >
						<ImageContent src="sap-icon://create-form"></ImageContent>
					</TileContent>
				</GenericTile>
				
				<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop"
					header="고객 목록 차트"
					press="onChartBP">
					<TileContent >
						<ImageContent src="sap-icon://donut-chart"></ImageContent>
					</TileContent>
				</GenericTile>
			</content>
		</Panel>
	</ScrollContainer>
</mvc:View>