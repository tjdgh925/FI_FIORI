<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">
    <ValueHelpDialog title="회사 코드"
        id="generalLedgerCompanyCodeDialog"
        ok=".onCompanyCodeOkPress"
        cancel=".onCompanyCodeCancelPress"
        supportRanges="true"
        key="CC_COMPANY_CODE"
        descriptionKey="ProductName">
        <filterBar>
            <fb:FilterBar advancedMode="true"
                search=".onCompnayCodeFilterBarSearch"
                isRunningInValueHelpDialog="true">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem groupName="__$INTERNAL$"
                        name="CC_COMPANY_CODE"
                        label="회사 코드"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="CC_COMPANY_CODE" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem groupName="__$INTERNAL$"
                        name="CC_COMPANY_NAME"
                        label="회사 이름"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="CC_COMPANY_NAME" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem groupName="__$INTERNAL$"
                        name="CC_CURRENCY"
                        label="통화"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="CC_CURRENCY" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem groupName="__$INTERNAL$"
                        name="CC_COA"
                        label="계정과목표"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="CC_COA" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>
