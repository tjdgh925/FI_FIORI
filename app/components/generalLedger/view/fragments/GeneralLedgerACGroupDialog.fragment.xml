<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">
    <ValueHelpDialog title="계정 그룹"
        id="generalLedgerACGroupDialog"
        ok="onACGroupOkPress"
        cancel="onACGroupCancelPress"
        supportRanges="true"
        key="AC_GROUP_CODE"
        descriptionKey="AC_GROUP">
        <filterBar>
            <fb:FilterBar advancedMode="true"
                search=".onAcGroupFilterBarSearch"
                isRunningInValueHelpDialog="true">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem groupName="__$INTERNAL$"
                        name="AC_GROUP_CODE"
                        label="계정 그룹 코드"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="AC Group Code" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem groupName="__$INTERNAL$"
                        name="AC_COA"
                        label="계정 과목표"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="AC COA" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem groupName="__$INTERNAL$"
                        name="AC_NAME"
                        label="계정 그룹 내역"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="AC Name" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>
